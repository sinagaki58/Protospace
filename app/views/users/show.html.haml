.container.user-page
  %header.user-nav
    .media
      .media-left
        %a{:href => "#"}
          %img.media-object{:alt => "64x64", "data-holder-rendered" => "true", "data-src" => "holder.js/64x64", :src => "#{current_user.avatar}", :style => "width: 64px; height: 64px;"}/
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree #{current_user.member}
        .row
          .col-md-6
            %h4 Profile
            %p #{current_user.profile}
          .col-md-6
            %h4 Works
            %p #{current_user.works}
.container.proto-list
  .row
    = render partial: "prototypes/prototype", collection: @prototypes
.text-center
  = paginate(@prototypes)
