= render "prototypes/header"
.container.proto-list
  .row
    - @tags.each do |tag|
      .col-sm-4.col-md-3.proto-content
        .thumbnail
          = link_to "#{ tag.name }", tag_path(tag.id, name: tag.name), class: "btn btn-tag"
          .tag-meta
            = link_to tag_path(tag.id), class: "label label-default tag-list-number" do
              %i #{ tag.taggings.count } Projects
